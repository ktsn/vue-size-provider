{
  "name": "vue-size-provider",
  "version": "0.2.1",
  "author": "katashin",
  "description": "Declarative element size observer and provider",
  "keywords": [
    "Vue",
    "size",
    "resize",
    "declarative",
    "scoped slot"
  ],
  "license": "MIT",
  "main": "dist/vue-size-provider.cjs.js",
  "module": "dist/vue-size-provider.esm.js",
  "unpkg": "dist/vue-size-provider.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "homepage": "https://github.com/ktsn/vue-size-provider",
  "bugs": "https://github.com/ktsn/vue-size-provider/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vue-size-provider.git"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist .tmp",
    "build": "node scripts/build.js",
    "build:test": "webpack --config scripts/webpack.config.test.js",
    "watch:test": "webpack -w --config scripts/webpack.config.test.js",
    "lint": "eslint \"@(src|test|scripts)/**/*.js\"",
    "lint:fix": "eslint --fix \"@(src|test|scripts)/**/*.js\"",
    "testem": "testem",
    "testem:ci": "testem ci",
    "dev": "run-p watch:test testem",
    "test": "run-s lint test:unit",
    "test:unit": "run-s build:test testem:ci",
    "storybook": "start-storybook -p 6006",
    "release": "run-s test clean build"
  },
  "devDependencies": {
    "@storybook/vue": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^4.19.0",
    "eslint-config-ktsn": "^2.0.1",
    "glob": "^7.1.2",
    "npm-run-all": "^4.1.2",
    "power-assert": "^1.4.4",
    "prettier": "^1.18.2",
    "prettier-config-ktsn": "^1.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "testem": "^2.0.0",
    "uglify-js": "^3.3.15",
    "vue": "^2.5.16",
    "vue-loader": "14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0"
  }
}
